<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.Exercise_Guide_Front">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_centerHorizontal="true">



        <ImageView
            android:id="@+id/Exersice_Guide_Front"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:src="@drawable/r" />


        <Button
            android:id="@+id/button_Neck"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:text="Neck"
            android:textColor="@color/white"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="@+id/Exersice_Guide_Front"
            app:layout_constraintEnd_toEndOf="@+id/Exersice_Guide_Front"
            app:layout_constraintHorizontal_bias="0.164"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="@+id/Exersice_Guide_Front"
            app:layout_constraintVertical_bias="0.036" />

        <Button
            android:id="@+id/button_Shoulders"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:text="Shoulders"
            android:textColor="@color/white"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="@+id/Exersice_Guide_Front"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.131"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="@+id/Exersice_Guide_Front"
            app:layout_constraintVertical_bias="0.175" />

        <Button
            android:id="@+id/button_Biceps"
            android:layout_width="90dp"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:text="Biceps"
            android:textColor="@color/white"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="@+id/Exersice_Guide_Front"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.105"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="@+id/Exersice_Guide_Front"
            app:layout_constraintVertical_bias="0.293" />

        <Button
            android:id="@+id/button_Abs"
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:textColor="@color/white"
            android:text="Abs"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="@+id/Exersice_Guide_Front"
            app:layout_constraintEnd_toEndOf="@+id/Exersice_Guide_Front"
            app:layout_constraintHorizontal_bias="0.078"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="@+id/Exersice_Guide_Front"
            app:layout_constraintVertical_bias="0.471" />

        <Button
            android:id="@+id/button_Quads"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:textColor="@color/white"
            android:text="Quads"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/Exersice_Guide_Front"
            app:layout_constraintHorizontal_bias="0.164"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="@+id/Exersice_Guide_Front"
            app:layout_constraintVertical_bias="0.607" />

        <Button
            android:id="@+id/button_Calves"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:textColor="@color/white"
            android:text="Calves"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="@+id/Exersice_Guide_Front"
            app:layout_constraintEnd_toEndOf="@+id/Exersice_Guide_Front"
            app:layout_constraintHorizontal_bias="0.879"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="@+id/Exersice_Guide_Front"
            app:layout_constraintVertical_bias="0.782" />

        <Button
            android:id="@+id/button_Forearms"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:text="Forearms"
            android:textColor="@color/white"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.879"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.597" />

        <Button
            android:id="@+id/button_Chest"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:text="Chest"
            android:textColor="@color/white"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/Exersice_Guide_Front"
            app:layout_constraintHorizontal_bias="0.916"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="@+id/Exersice_Guide_Front"
            app:layout_constraintVertical_bias="0.219" />

        <Button
            android:id="@+id/button_Traps"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:background="@drawable/button_black"
            android:text="Traps"
            android:textColor="@color/white"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/Exersice_Guide_Front"
            app:layout_constraintHorizontal_bias="0.879"
            app:layout_constraintStart_toStartOf="@+id/Exersice_Guide_Front"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.081" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>